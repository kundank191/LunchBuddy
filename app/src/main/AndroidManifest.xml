<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suliteos.android.lunchbuddy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_lunch_break"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_lunch_break"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.suliteos.android.lunchbuddy.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.suliteos.android.lunchbuddy.ProfileActivity" />
        <activity android:name="com.suliteos.android.lunchbuddy.ChatActivity" />
        <activity android:name="com.suliteos.android.lunchbuddy.MainActivity"/>
    </application>

</manifest>